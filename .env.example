# Reolink Device Credentials
# Copy this file to .env and update with your actual values

# Camera/NVR IP address
REOLINK_NVR_HOST=192.168.1.100

# Camera/NVR username (default is usually 'admin')
REOLINK_NVR_USER=admin

# Camera/NVR password
REOLINK_NVR_PASS=your_password

# Optional: Webhook URL for event notifications (used in example 05)
WEBHOOK_URL=http://localhost:3001/webhook
